			<script>

	var container = document.createElement( "div" );
	container.className = "knobContainer";
	container.style.left = "100px";
	container.style.top = "100px";
	container.style.width="100px";
	container.style.height="100px";


	document.createElement( "webaudio-knob" );
	knob.id = "osc2detune";
	knob.setAttribute( "value", "" + currentOsc2Detune );
	knob.setAttribute( "src", "controls/img/MiniMoogFrequency.png" );
	knob.setAttribute( "min", "-1200" );
	knob.setAttribute( "max", "+1200");
	knob.setAttribute("units", "Cent");
	knob.setAttribute( "diameter", "45" );
	knob.setAttribute( "sprites", "30" );
	knob.ready();
	knob.onchange = updateOsc2Detune;

	container.appendChild( knob );

			</script>


